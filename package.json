{
  "name": "vancedvue",
  "version": "1.0.9",
  "description": "Fully styled and customizable components for Vue 3",
  "author": "suralabs",
  "main": "./dist/vancedvue.umd.js",
  "module": "./dist/vancedvue.es.js",
  "exports": {
    ".": {
      "import": "./dist/vancedvue.es.js",
      "require": "./dist/vancedvue.umd.js"
    }
  },
  "unpkg": "./dist/vancedvue.umd.js",
  "jsdelivr": "./dist/vancedvue.umd.js",
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suralabs/vancedvue.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/suralabs/vancedvue/issues"
  },
  "homepage": "vancedvue.vercel.app",
  "scripts": {
    "build": "vitepress build docs",
    "dist": "vite build -c build/vite.config.js && node build/single.js",
    "lint": "eslint --ext .js,.vue src build docs/.vitepress",
    "lint:fix": "npm run lint -- --fix",
    "docs:preview": "vitepress preview docs",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs"
  },
  "devDependencies": {
    "@babel/preset-env": "7.23.3",
    "@vitejs/plugin-vue": "4.5.0",
    "@vitejs/plugin-vue-jsx": "3.1.0",
    "@vue/babel-plugin-jsx": "1.1.5",
    "@vue/compiler-sfc": "3.3.8",
    "@vue/test-utils": "2.4.2",
    "@vue/vue3-jest": "29.2.6",
    "autoprefixer": "^10.4.16",
    "axios": "1.6.2",
    "babel-core": "^4.7.16",
    "babel-jest": "29.7.0",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-html": "7.1.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-vue": "9.18.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-serializer-vue": "3.1.0",
    "less": "4.2.0",
    "lodash": "4.17.21",
    "postcss": "^8.4.31",
    "prettier": "3.1.0",
    "sinon": "17.0.1",
    "tailwindcss": "^3.3.5",
    "vite": "5.0.0",
    "vitepress": "^1.0.0-rc.27",
    "vue": "3.3.8",
    "vue-i18n": "9.7.0",
    "vuepress": "^1.9.10"
  },
  "peerDependencies": {
    "vue": "^3"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/__test__/jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "test/"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*",
      "!src/__test__/**/*",
      "!**/*.snap"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "@vue/vue3-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "customExportConditions": [
        "node",
        "node-addons"
      ]
    }
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [
    "Tailwind",
    "Tailwindcss",
    "Tailwindcss for Vue",
    "SSR",
    "Web",
    "Components",
    "Directives",
    "Vue",
    "VueJS",
    "Vue3",
    "WebComponents",
    "vue-strap",
    "vuestrap",
    "CSS",
    "Notification",
    "Alert",
    "Button",
    "Modal",
    "Navbar",
    "Pagination",
    "Popover",
    "Progress",
    "Tab",
    "Tooltip",
    "Typeahead",
    "Auto complete",
    "Multi select"
  ]
}
